<config>
    <database>
        <name value="mssql" key="MicroSoft SqlServer">
            <versions>
                <version name="2000">
                    <drivers>
                        <driver name="com.microsoft.jdbc.sqlserver.SQLServerDriver">
                            <vender>MICROSOFT</vender>
                            <defaultport>1433</defaultport>
                            <url>jdbc:microsoft:sqlserver://{0}:{1};DatabaseName={2}</url>
                            <catalog></catalog>
                        </driver>
                        <driver name="net.sourceforge.jtds.jdbc.Driver">
                            <vender>JTDS</vender>
                            <defaultport>1433</defaultport>
                            <url>jdbc:jtds:sqlserver://{0}:{1}/{2}</url>
                            <catalog></catalog>
                        </driver>
                    </drivers>
                </version>
