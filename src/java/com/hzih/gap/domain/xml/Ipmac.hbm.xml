<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="ipmac" name="com.hzih.gap.domain.Ipmac">
        <id access="field" name="id" type="int" column="id" length="10">
            <generator class="increment"/>
        </id>
        <property name="ip" length="20" column="ip" access="field" type="string"/>
        <property name="mac" length="20" column="mac" access="field" type="string"/>
        <property name="probe" length="1" column="probe" access="field" type="int"/>
    </class>
</hibernate-mapping>
